{
  "entrypoint": "mapbox-gl.html",
  "fragments": [
    "mapbox-gl-marker.html",
    "mapbox-layer.html",
    "mapbox-building-layer.html",
    "mapbox-heatmap-layer.html",
    "geojson-source.html"
  ],
  "sources": [
    "src/**/*",
    "mapbox-gl.html",
    "mapbox-gl-marker.html",
    "mapbox-layer.html",
    "mapbox-building-layer.html",
    "mapbox-heatmap-layer.html",
    "geojson-source.html"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js"
  ],
  "builds": [
    {
      "name": "es5",
      "js": {
        "minify": true,
        "compile": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}